#include <stdio.h>

int main()
{
  int a = 1;
  while(a<99)
  {
    if(a%2 == 1)
    {
      printf("%d, ", a);
      a++;
    }
    else
    {
      a++;
    }
  }
  while(a<=100)
  {
    if(a%2 == 1)
    {
      printf("%d\n", a);
      a++;
    }
    else
    {
      a++;
    }
  }
  return 0;
}
